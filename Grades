#include <stdio.h>
int main()
{
    int score=0;
    printf("Enter the Score= ");
    scanf("%d",&score);
    
    if(score>=85 && score<=100)
   {
       printf("Grade A");
   }
   if(score>=70 && score<=84)
   {
      printf("Grade B"); 
   }
   if(score>=55 && score<=69)
   {
      printf("Grade C"); 
   }
   if(score>=40 && score<=54)
   {
      printf("Grade D"); 
   }
   if(score<40)
   {
      printf("Grade F"); 
   }
}
